<?php
namespace ev1;

$cadena='Alberto:Garay:913334455//Ana:Garcia:914445566';
$personas=explode('//',$cadena);

foreach ($personas as $elemento){
    
    $datos = explode(':',$elemento);
    echo "\n".'Nombre: '.$datos[0]."\n";
    echo 'Apellido: '.$datos[1]."\n";
    echo 'TelÃ©fono: '.$datos[2]."\n";
    echo '============';    
}



?>
